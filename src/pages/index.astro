---
import { AboutSection, ContactSection, FeaturedSection } from "@/components";
import GamesSection from "@/components/sections/games-section.astro";
import { home } from "@/data";
import Layout from "@/layouts/Layout.astro";
---

<Layout title="TGD Studio | Home">
  <main class="w-full">
    <FeaturedSection featuredGame={home["featured-game"]} />
    <GamesSection games={home["games"]} />
    <AboutSection aboutData={home["about"]} client:load />
    <ContactSection contactData={home["contact"]} />
  </main>
</Layout>
